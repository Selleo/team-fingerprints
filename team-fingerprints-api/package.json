    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@types/cache-manager-redis-store": "^2.0.1",
    "axios": "^0.24.0",
    "bull": "^4.8.1",
    "cache-manager": "^3.6.1",
